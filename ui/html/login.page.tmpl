{{template "base" .}}
{{define "title"}}Login{{end}}
{{define "body"}}
 
<form action='/login' method='POST' novalidate>
    <div>
        <label>Email:</label>
        <input type='email' name='username'>
    </div>
    <div>
        <label>Password:</label>
        <input type='password' name='password'>
    </div>
    <div>
        <input type='submit' value='Login'>
    </div>
</form>
{{end}}